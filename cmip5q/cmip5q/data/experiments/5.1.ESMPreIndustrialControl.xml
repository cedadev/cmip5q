<?xml version="1.0" encoding="UTF-8"?>
<numericalExperiment documentVersion="3" control="true"
    xmlns="http://www.metaforclimate.eu/cim/1.1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.metaforclimate.eu/cim/1.1 C:\Users\clp73\Desktop\Metafor\subversion\CIM\branches\dev0p3\activity.xsd"
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xmlns:gmd="http://www.isotc211.org/2005/gmd"
    xmlns:gco="http://www.isotc211.org/2005/gco">
    
    <rationale>Control for exploring the impact of climate-carbon cycle coupling in 
    quantifying the carbon cycle feedback on projected climated change.</rationale>
    <description>
        Impose non-evolving pre-industrial conditions as in experiment 3.1_piControl
        but emissions-forced (with atmosperhic CO2 determined by the model itself)
    </description>
    
    <!-- Initial Conditions -->
    <numericalRequirement xsi:type="InitialCondition">
        <id xsi:type="Identifier">ic.005</id>
        <name xsi:type="Identifier">5.1.ic</name>
        <description>
            A description of initial condition for the ESM control run.
        </description>
    </numericalRequirement>
    
    <!-- Boundary Conditions -->
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.044</id>
        <name xsi:type="Identifier">5.1.bc.wmg.em</name>
        <description>Prescribed atmospheric emissions of pre-industrial well mixed gases: excluding CO2</description>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.045</id>
        <name xsi:type="Identifier">5.1.bc.CO2.em</name>
        <description>Prescribed atmospheric emissions of pre-industrial well mixed gas: Carbon Dioxide</description>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.046</id>
        <name xsi:type="Identifier">5.1.bc.sls.em</name>
        <description>Prescribed atmospheric emissions of pre-industrial short lived (reactive) gas
            species</description>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.007</id>
        <name xsi:type="Identifier">5.1.bc.nat_aer.em</name>
        <description>Prescribed emissions of pre-industrial natural aerosols</description>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.008</id>
        <name xsi:type="Identifier">5.1.bc.nat_aer_precur.em</name>
        <description>Prescribed emissions of pre-industrial natural aerosol precursors</description>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.009</id>
        <name xsi:type="Identifier">5.1.bc.sl_aer.em</name>
        <description>Prescribed emissions of pre-industrial short lived (reactive) aerosol
            species</description>
    </numericalRequirement>
    <numericalRequirement xsi:type="BoundaryCondition">
        <id xsi:type="Identifier">bc.010</id>
        <name xsi:type="Identifier">5.1.bc.LandUse</name>
        <description>Unperturbed Pre-Industrial Land Use</description>
    </numericalRequirement>
    
    <shortName>
		5.1 ESM control
	</shortName>
    <longName>
		ESM pre-industrial control
	</longName>
    
    <Calendar>
        <perpetualPeriod/>
    </Calendar>
    <requiredDuration xsi:type="ClosedDateRange">
        <startDate>1850-01-01T00:00:00Z</startDate>
        <endDate>1850-30-12T00:00:00Z</endDate>
        <lengthYears>250</lengthYears>
    </requiredDuration>
    <author>
        <gmd:CI_ResponsibleParty>
            <gmd:individualName>
                <gco:CharacterString> Charlotte Pascoe </gco:CharacterString>
            </gmd:individualName>
            <gmd:organisationName>
                <gco:CharacterString> BADC, CEDA, STFC </gco:CharacterString>
            </gmd:organisationName>
            <gmd:contactInfo>
                <gmd:CI_Contact>
                    <gmd:address>
                        <gmd:CI_Address>
                            <gmd:electronicMailAddress>
                                <gco:CharacterString> charlotte.pascoe@stfc.ac.uk
                                </gco:CharacterString>
                            </gmd:electronicMailAddress>
                        </gmd:CI_Address>
                    </gmd:address>
                </gmd:CI_Contact>
            </gmd:contactInfo>
            <gmd:role>
                <gmd:CI_RoleCode
                    codeListValue="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#CI_RoleCode"
                    codeList="originator"/>
            </gmd:role>
        </gmd:CI_ResponsibleParty>
    </author>
    <creationDate>2009-08-04T13:14:00Z</creationDate>
    <documentID xsi:type="Identifier">
        5.1_esmControl
    </documentID>
    
</numericalExperiment>
